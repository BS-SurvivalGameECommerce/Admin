n  },\n  mutations: {\n    SET_STOCK(state, data) {\n      state.stock = data\n      LS.save(state.stock)\n      console.log('set', state.stock)\n    },\n    ADD_STOCK(state, data) {\n      state.stock.push(data)\n      // axios\n      //   .post('/list.json', data)\n      //   .then((res) => {\n      //     state.stock.push(res.data)\n      //   })\n      //   .catch((err) => {\n      //     console.log(err)\n      //   })\n      LS.save(state.stock)\n      console.log('add', state.stock)\n    },\n    UPDATE_STOCK(state, { id, data }) {\n      let index = state.stock.findIndex((x) => x.ID == id)\n      console.log('update - data', data)\n      state.stock[index] = data\n      console.log('update', state.stock)\n      LS.save(state.stock)\n    },\n    REMOVE_STOCK(state, id) {\n      let index = state.stock.findIndex((x) => x.ID == id)\n      state.stock.splice(index, 1)\n      // axios\n      //   .delete('/list.json/' + id)\n      //   .then((res) => {\n      //     state.stock.splice(index, 1)\n      //   })\n      //   .catch((err) => {\n      //     console.log(err)\n      //   })\n      LS.save(state.stock)\n    },\n    // SETLIST(state, list) {\n    //   state.list = list\n    // },\n  },\n  actions: {\n    // 從api取得資料\n    INIT_STOCK(context) {\n      return axios.get(api).then((res) => {\n        context.commit('SET_STOCK', res.data.data)\n      })\n    },\n    // INIT_STOCK({ commit }) {\n    //   commit('SET_STOCK', LS.load)\n    // },\n    // GETLIST(context) {\n    //   console.log('action-context', context)\n\n    //   return axios.get('/list.json').then((res) => {\n    //     context.commit('SETLIST', res.data)\n    //   })\n    // },\n  },\n  modules: {},\n})\n"],"sourceRoot":""}]}